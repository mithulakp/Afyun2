.spec-table,
    .spec-table thead,
    .spec-table tbody,
    .spec-table th,
    .spec-table td,
    .spec-table tr {
        display: block;
    }
    .spec-table thead tr {
        position: absolute;
        top: -9999px;
        left: -9999px;
    }
    .spec-table tr {
        border: 1px solid #000;
    }
    .spec-table td {
        border: none;
        border-bottom: 1px solid #000;
        position: relative;
        padding-left: 50%;
    }
    .spec-table td:before {
        position: absolute;
        top: 6px;
        left: 4px;
        width: 45%;
        padding-right: 10px;
    }
    .spec-table td:nth-of-type(1):before {
        content: "TEST";
    }
    .spec-table td:nth-of-type(2):before {
        content: "ISI SPECIFIED VALUE";
    }
    .spec-table td:nth-of-type(3):before {
        content: "RESULT OBTAINED";
    }
    .spec-table .none:nth-of-type(1):before {
        content: none;
    }
    .sub-text {
        text-align: center;
    }
    .bg-tr {
        background-color: black !important;
    }
    .bg-gray-tr {
        background-color: #131313;
    }
    .spec-table tr {
        border: none;
    }
    .spec-table td {
        border-bottom: none;
    }
